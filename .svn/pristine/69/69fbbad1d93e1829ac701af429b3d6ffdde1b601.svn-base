<form id="add-form" class="form-horizontal" role="form" data-toggle="validator" method="POST" action="">

    <div class="form-group">
        <label for="c-unid" class="control-label col-xs-12 col-sm-2">所有渠道列表:</label>
        <div class="col-xs-12 col-sm-8">
            <select  id="unid" data-rule="required" class="form-control selectpicker" name="unid">
                {foreach name="selectStoreList" item="vo"}
                <option value="{$key}" {in name="key" value=""}selected{/in}>{$vo.unid_name}</option>
                {/foreach}
            </select>
        </div>
    </div>

    <div class="form-group layer-footer">
        <label class="control-label col-xs-12 col-sm-2"></label>
        <div class="col-xs-12 col-sm-8">
            <button type="submit" class="btn btn-success btn-embossed disabled">{:__('OK')}</button>
            <button type="reset" class="btn btn-default btn-embossed">{:__('Reset')}</button>
        </div>
    </div>
</form>